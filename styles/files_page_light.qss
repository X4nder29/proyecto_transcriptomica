#SelectInputsWidget, #SelectDatabaseWidget {
    border: 2px dashed #6C757D;
    border-radius: 5px;
}

#PreviousReportsLabel, #SelectDatabaseLabel {
    font-size: 14px;
    font-weight: bold;
    color: #212529;
}

#NavigationButton {
    background-color: transparent;
    border: 2px solid #49607b;
    border-radius: 5px;
    padding: 10px 20px;
    font-weight: bold;
    color: #49607b;
}

#NavigationButton:hover {
    background-color: #d0e4ff;
}