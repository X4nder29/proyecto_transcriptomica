#HeaderLabel {
    font-size: 14px;
    font-weight: bold;
    color: #324b49;
}

#UploadButton {
    color: #ffffff;
	border: none;
	border-radius: 5px;
	padding: 5px;
}

#UploadButton:hover {
	background-color: #324b49;
}

#FilterButton {
    background-color: #FFFFFF;
    border: none;
    border-radius: 5px;
    padding: 7.5px;
    color: #FFFFFF;
}

#FilterButton:!checked:hover {
    background-color: #80d5cf;
}

#FilterButton:!checked {
    color: #212529;
}

#FilterButton:checked {
    background-color: #006a66;
    color: #FFFFFF;
}