#HelpButton {
    background-color: none;
    border: none;
    color: white;
    font-size: 12px;
    padding: 2px 5px;
    border-radius: 5px;
}

#HelpButton:hover {
    background-color: #505050;
}

QWidget#IlluminaClipOption {
    background-color: #404040;
    border-radius: 10px;
}

#Title {
    font-size: 14px;
    font-weight: bold;
    color: white;
}

#AdapterLabel {
    font-size: 12px;
    color: white;
}
#AdapterLabel:disabled {
    color: #909090;
}

#AdapterOptions {
    border-radius: 5px;
    padding: 5px;
}

#SeedMismatchesLabel {
    font-size: 12px;
    color: white;
}
#SeedMismatchesLabel:disabled {
    color: #909090;
}

#PalindromeClipThresholdLabel {
    font-size: 12px;
    color: white;
}
#PalindromeClipThresholdLabel:disabled {
    color: #909090;
}

#ActiveButton {
    background-color: none;
    border: none;
    width: 16;
    height: 16;
}

#ActiveButton:hover {
    background-color: #505050;
}