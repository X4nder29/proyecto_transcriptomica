#SearchLineEdit {
    background-color: transparent;
    border: none;
    font-size: 14px;
    color: black;
}

#CreateNewWorkspaceButton, #OpenExistingWorkspaceButton {
    border: 1px solid #6C757D;
    border-radius: 5px;
    padding: 5px 10px;
    background-color: #6C757D;
    color: white;
}

#CreateNewWorkspaceButton:hover, #OpenExistingWorkspaceButton:hover {
    background-color: #5A6268;
}

#HorizontalSpacer {
    color: black;
    max-height: 1px;
}


#WorkspacesScrollArea {
    background-color: #F3F3F3;
    border: none;
}

#WorkspacesList {
    background-color: #F3F3F3;
}