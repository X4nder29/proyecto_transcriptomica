#InstalledLabel, #UninstalledLabel {
    font-size: 16px;
    font-weight: bold;
    color: #212529;
}

#LinkLineEdit {
    border: none;
    border-radius: 5px;
    padding: 10px;
    background-color: #3578E5;
    color: #FFFFFF;
}

#AddButton {
    background-color: #3578E5;
    color: white;
    border: none;
    border-radius: 5px;
    padding: 10px;
}

#AddButton:hover {
    background-color: #2F69CC;
}
